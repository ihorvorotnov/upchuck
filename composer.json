{
    "name": "bkwld/upchuck",
    "description": "A simple, automatic handler of file uploads for Laravel's Eloquent models using using Flysystem.",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Reinhard",
            "email": "info@bkwld.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0",
        "illuminate/queue": "^6.0",
        "illuminate/support": "^6.0",
        "graham-campbell/flysystem": "^6.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "conflict": {
        "laravel/framework": "<6.0"
    },
    "suggest": {
        "league/flysystem-cached-adapter": "Required for Flysystem caching of remote disks"
    },
    "autoload": {
        "psr-4": {
            "Bkwld\\Upchuck\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkwld\\Upchuck\\ServiceProvider"
            ]
        }
    }
}
